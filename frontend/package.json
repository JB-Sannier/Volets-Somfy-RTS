{
  "name": "somfyrts-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:lan": "vite -c vite.config.lan.ts",
    "start:wan": "vite -c vite.config.wan.ts",
    "build": "tsc -b  && vite build -c vite.config.lan.ts",
    "build:lan": "tsc --project tsconfig.lan.json && vite build -c vite.config.lan.ts",
    "build:wan": "tsc --project tsconfig.wan.json && vite build -c vite.config.wan.ts",
    "lint": "eslint src/",
    "preview": "vite preview",
    "format": "prettier --config ./.prettierrc.json --write ./src"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "axios": "^1.13.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.0",
    "react-jwt": "^2.0.0",
    "react-router-dom": "^7.11.0"
  },
  "scarfSettings": {
    "enabled": false
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0"
  }
}
