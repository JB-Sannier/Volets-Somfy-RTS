{
  "name": "somfyrts-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:lan": "vite -c vite.config.lan.ts",
    "start:wan": "vite -c vite.config.wan.ts",
    "build": "tsc -b  && vite build -c vite.config.lan.ts",
    "build:lan": "tsc --project tsconfig.lan.json && vite build -c vite.config.lan.ts",
    "build:wan": "tsc --project tsconfig.wan.json && vite build -c vite.config.wan.ts",
    "lint": "eslint src/",
    "preview": "vite preview",
    "format": "prettier --config ./.prettierrc.json --write ./src"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.11.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-i18next": "^15.6.1",
    "react-jwt": "^1.3.0",
    "react-router-dom": "^7.7.1"
  },
  "scarfSettings": {
    "enabled": false
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0"
  }
}
